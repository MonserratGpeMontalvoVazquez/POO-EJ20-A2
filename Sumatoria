#include <iostream>
using namespace std;
int main() {

	int n1, n2, n3, n4, n5, n6, n7, n8, n9, n10;
	cout << "Inserte 10 numeros y te dare la sumatoria:" << endl;
	cout << "Inserte numero aqui: " << endl;
	cin >> n1;
	cout << "Inserte numero aqui: " << endl;
	cin >> n2;
	cout << "Inserte numero aqui: " << endl;
	cin >> n3;
	cout << "Inserte numero aqui: " << endl;
	cin >> n4;
	cout << "Inserte numero aqui: " << endl;
	cin >> n5;
	cout << "Inserte numero aqui: " << endl;
	cin >> n6;
	cout << "Inserte numero aqui: " << endl;
	cin >> n7;
	cout << "Inserte numero aqui: " << endl;
	cin >> n8;
	cout << "Inserte numero aqui: " << endl;
	cin >> n9;
	cout << "Inserte numero aqui: " << endl;
	cin >> n10;

	cout << "	La Sumatoria es: " << n1 + n2 + n3 + n4 + n5 + n6 + n7 + n8 + n9 + n10 << endl;
	return 0;
}
